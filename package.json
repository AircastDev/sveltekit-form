{
	"name": "sveltekit-form",
	"version": "0.1.0",
	"license": "MIT",
	"description": "A progressively enhanced form for SvelteKit, similar to Remix's <Form>",
	"repository": {
		"type": "git",
		"url": "https://github.com/AircastDev/sveltekit-form"
	},
	"bugs": "https://github.com/AircastDev/sveltekit-form/issues",
	"homepage": "https://github.com/AircastDev/sveltekit-form#readme",
	"author": "James Birtles <james@birtles.dev>",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.5.9",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"zod": "^3.14.4"
	}
}
